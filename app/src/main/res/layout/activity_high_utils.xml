<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:itheima="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <TextView
        style="@style/Title"
        android:text="常用工具"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <heima.it.safe.view.MySettingView
        android:id="@+id/highutils_address"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        itheima:biaoti="号码归属地查询"
        itheima:mybutton="false"
        itheima:img="first"
        />
    <heima.it.safe.view.MySettingView
        android:id="@+id/highutils_findphone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        itheima:biaoti="常用号码查询"
        itheima:mybutton="false"
        itheima:img="last"
        />
</LinearLayout>
